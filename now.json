{
  "version": 2,
  "name": "go",
  "public": true,
  "builds": [
    {
      "src": "api/**/*.go",
      "use": "@now/go"
    }
  ],
  "build": {
    "env": {
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  },
  "routes": [
    {
      "src": "/dispatch",
      "dest": "api/dispatch.go"
    }
  ]
}